{
  "compilerOptions": {
    "target": "ES2015",
    "module": "esnext",
    "jsx": "react",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "types": ["react", "react-native"]
  },
  "include": [
    "src/**/*",
    "App.tsx",
    "index.js"
  ]
}
